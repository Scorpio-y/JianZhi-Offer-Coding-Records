import java.util.Scanner;

// 《剑指offer》第3题：数组中重复的数字
public class Main{
	public static void main(String[] args) {
		// 接收键盘输入的数组
		Scanner sc = new Scanner(System.in);
		int len = sc.nextInt();
		int[] arr = new int[len];
		for(int i=0;i<len;i++) {
			arr[i] = sc.nextInt();
		}
		
		System.out.println(duplicate(arr));
	}
	
	// 判断数组中是否有重复的数字
	private static boolean duplicate(int[] arr) {
		int len = arr.length;
		for(int i=0;i<len;i++) {
			while(arr[i]!=i) {
				if(arr[arr[i]]!=arr[i]) {
					int tmp = arr[arr[i]];
					arr[arr[i]] = arr[i];
					arr[i] = tmp;
				}else {
					return true;
				}
			}
		}
		return false;
	}
	
}